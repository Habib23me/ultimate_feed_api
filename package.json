{
    "name": "ultimate-feed",
    "version": "0.0.1",
    "description": "",
    "author": "Nathan Getachew",
    "private": true,
    "license": "Proprietary",
    "scripts": {
        "postbuild": "copyfiles --up 1 src/**/*.json dist",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main.js",
        "start": "nest start",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "NODE_ENV=test jest",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:cov": "NODE_ENV=test jest --coverage",
        "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -f ormconfig",
        "migration:generate": "npm run typeorm migration:generate -d src/database/migrations -n",
        "migration:create": "npm run typeorm migration:create -d src/database/migrations -n",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert",
        "typeorm-seeding": "ts-node -r tsconfig-paths/register node_modules/typeorm-seeding/dist/cli.js",
        "seed:config": "npm run typeorm-seeding config",
        "seed:run": "npm run typeorm-seeding seed",
        "schema:drop": "npm run typeorm schema:drop"
    },
    "dependencies": {
        "@moneteam/eslint-plugin-nestjs": "^1.5.2",
        "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.1",
        "@nestjs/axios": "0.0.7",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/cqrs": "^8.0.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^8.4.4",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/terminus": "^8.0.6",
        "@nestjs/typeorm": "^8.0.3",
        "@types/compression": "^1.7.2",
        "@types/helmet": "^4.0.0",
        "@types/morgan": "^1.9.3",
        "@types/webpack-env": "^1.16.3",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "clean-webpack-plugin": "^4.0.0",
        "compression": "^1.7.4",
        "express-ctx": "^0.1.1",
        "express-rate-limit": "^6.3.0",
        "helmet": "^5.0.2",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "swagger-ui-express": "^4.3.0",
        "ts-node-dev": "^1.1.8",
        "typeorm": "^0.2.45",
        "typeorm-transactional-cls-hooked": "^0.1.21",
        "uuid": "^8.3.2",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-node-externals": "^3.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "27.4.1",
        "@types/lodash": "^4.14.181",
        "@types/node": "^16.0.0",
        "@types/passport-jwt": "^3.0.6",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-helpers": "^1.2.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sonarjs": "^0.13.0",
        "eslint-plugin-unicorn": "^42.0.0",
        "jest": "^27.2.5",
        "joi": "^17.6.0",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typeorm-seeding": "^1.6.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
